<%= link_to "Air Park", root_path, :class => 'brand' %>
<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
  <span class="icon-bar"></span>
  <span class="icon-bar"></span>
  <span class="icon-bar"></span>
</a>
<div class="nav-collapse collapse">
<ul class="nav">
  <% if user_signed_in? %>
    <li>
    <%= link_to 'Logout', destroy_user_session_path, :method=>'delete' %>        
    </li>
  <% else %>
    <li>
    <%= link_to 'Login', new_user_session_path %>  
    </li>
  <% end %>
  <% if user_signed_in? %>
    <li>
    <%= link_to 'Edit account', edit_user_registration_path %>
    </li>
    <% if can? :read, @user %>
      <li>
      <%= link_to 'Users', users_path %>
      </li>
    <% end %>
    <% if can? :read, @company %>
      <li class="dropdown">
      <a href="#" class="dropdown-toggle" data-toggle="dropdown">Companies <b class="caret"></b></a>
      <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
        <% if can? :read, @company %>
        <li>
          <%= link_to 'List All', all_companies_path %>
        </li>
        <% end %>
        <% if can? :create, @company %>
        <li>
          <%= link_to 'Add New Company', add_parking_company_path %>
        </li>
        <% end %>
        <li>
          <%= link_to 'Performance Report', "#" %>
        </li>
        <li class="divider"></li>
        <% if can? :create, @branch %>
        <li class="nav-header">Branches</li>
        <li>
          <%= link_to 'Add New Branch', add_parking_branch_path %>
        </li>
        <% end %>

      </ul>
      </li>

    <% end %>
    <% if can? :read, @Feedback %>
      <li>
      <%= link_to 'Feedbacks', feedbacks_path %>
      </li>
    <% end %>
    <% if can? :read, @Car %>
      <li>
      <%= link_to 'Cars', cars_path %>
      </li>
    <% end %>
    <% if current_user.has_role? :admin %>
      <li>
      <%= link_to 'Current Bookings', current_bookings_path %>
      </li>
    <% end %>
  <% else %>
    <li>
    <%= link_to 'Sign up', new_user_registration_path %>
    </li>
  <% end %>
</ul>
</div>