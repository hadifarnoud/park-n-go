<%= semantic_form_for @booking do |f| %>
  <%= f.inputs do %>
  	<%= f.input :transaction_id %>
  	<%= f.input :user_id %>
  	<%= f.input :car_id %>
  	<%= f.input :pick_up, :as => :string %>
  	<%= f.input :drop_off, :as => :string  %>
  	<%= f.input :flight_inbound %>
  	<%= f.input :flight_outbound %>
  	<%= f.input :where_i_parked %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
